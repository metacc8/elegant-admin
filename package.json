{
	"name": "elegant-admin",
	"private": true,
	"version": "1.0.0",
	"description": "Elegant-Admin后台管理系统",
	"author": "zhangyao <97972619@qq.com>",
	"scripts": {
		"dev": "vite",
		"build:test": "vue-tsc --noEmit && vite build --mode tests",
		"build:sit": "vue-tsc --noEmit && vite build --mode sit",
		"build:pro": "vue-tsc --noEmit && vite build --mode production",
		"serve:test": "http-server ./dist-tests -o",
		"svgo": "svgo -f src/assets/icons",
		"new": "plop",
		"lint:eslint": "eslint src/**/*.{ts,tsx,vue} --fix",
		"lint:prettier": "prettier --write --loglevel warn \"src/**/*.{js,ts,json,tsx,css,less,scss,vue,html,md}\"",
		"lint:stylelint": "stylelint src/**/*.{css,scss,vue} --fix",
		"lint:lint-staged": "lint-staged",
		"prepare": "husky install",
		"postinstall": "esno scripts/prepare.js",
		"preinstall": "npx only-allow pnpm",
		"commit": "git pull && git add -A && git-cz && git push"
	},
	"dependencies": {
		"@tinymce/tinymce-vue": "^5.0.0",
		"@vueuse/core": "^9.3.0",
		"@better-scroll/core": "^2.4.2",
		"@vueuse/integrations": "^9.3.0",
		"axios": "^0.27.2",
		"dayjs": "^1.11.5",
		"crypto-js": "^4.1.1",
		"@element-plus/icons-vue": "^2.0.10",
		"element-plus": "^2.2.17",
		"hotkeys-js": "^3.10.0",
		"lodash-es": "^4.17.21",
		"mitt": "^3.0.0",
		"nprogress": "^0.2.0",
		"path-browserify": "^1.0.1",
		"path-to-regexp": "^6.2.1",
		"pinia": "^2.0.23",
		"qs": "^6.11.0",
		"print-js": "^1.6.0",
		"tinymce": "^6.2.0",
		"vue": "^3.2.40",
		"vue-router": "^4.1.5",
		"css-render": "0.15.10",
		"ua-parser-js": "^1.0.2"
	},
	"devDependencies": {
		"@typescript-eslint/eslint-plugin": "^5.22.0",
		"@typescript-eslint/parser": "^5.22.0",
		"@types/ua-parser-js": "^0.7.36",
		"@iconify-json/ep": "^1.1.8",
		"@iconify/vue": "^4.0.0",
		"@types/nprogress": "^0.2.0",
		"@types/path-browserify": "^1.0.0",
		"@types/qs": "^6.9.7",
		"@types/node": "^18.7.18",
		"@types/crypto-js": "^4.1.1",
		"@vitejs/plugin-vue": "^3.1.2",
		"@vitejs/plugin-vue-jsx": "^2.0.1",
		"@commitlint/cli": "^17.1.2",
		"@commitlint/config-conventional": "^17.1.0",
		"commitizen": "^4.2.4",
		"commitlint-config-cz": "^0.13.2",
		"tailwindcss": "^3.2.3",
		"autoprefixer": "^10.4.12",
		"cz-conventional-changelog": "^3.3.0",
		"cz-customizable": "^7.0.0",
		"eslint": "^8.25.0",
		"eslint-config-prettier": "^8.5.0",
		"eslint-plugin-prettier": "^4.0.0",
		"eslint-plugin-vue": "^8.7.1",
		"prettier": "^2.6.2",
		"esno": "^0.16.3",
		"fs-extra": "^10.1.0",
		"http-server": "^14.1.1",
		"husky": "^8.0.1",
		"lint-staged": "^13.0.3",
		"plop": "^3.1.1",
		"postcss-html": "^1.5.0",
		"postcss": "^8.4.14",
		"sass": "^1.55.0",
		"stylelint": "^14.8.5",
		"stylelint-config-html": "^1.0.0",
		"stylelint-config-prettier": "^9.0.3",
		"stylelint-config-recess-order": "^3.0.0",
		"stylelint-config-recommended-scss": "^6.0.0",
		"stylelint-config-recommended-vue": "^1.4.0",
		"stylelint-config-standard": "^25.0.0",
		"stylelint-config-standard-scss": "^3.0.0",
		"svgo": "^2.8.0",
		"typescript": "^4.8.4",
		"unplugin-auto-import": "^0.11.2",
		"unplugin-vue-components": "^0.22.8",
		"vite": "^3.1.7",
		"vite-plugin-banner": "^0.5.0",
		"vite-plugin-compression": "^0.5.1",
		"vite-plugin-html": "^3.2.0",
		"vite-plugin-pages": "^0.26.0",
		"vite-plugin-restart": "^0.2.0",
		"vite-plugin-spritesmith": "^0.1.1",
		"vite-plugin-eslint": "^1.6.0",
		"vite-plugin-svg-icons": "^2.0.1",
		"vite-plugin-vue-layouts": "^0.7.0",
		"vite-plugin-vue-setup-extend": "^0.4.0",
		"vue-tsc": "^1.0.3"
	},
	"pnpm": {
		"peerDependencyRules": {
			"ignoreMissing": [
				"rollup"
			]
		}
	},
	"config": {
		"commitizen": {
			"path": "node_modules/cz-customizable"
		}
	},
	"husky": {
		"hooks": {
			"commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
		}
	}
}
